{
	"name": "datav-expo-directlighter",
	"version": "0.0.3",
	"main": "index.js",
	"dependencies": {
		"chroma-js": "1.3.4",
		"safely-merge": "1.0.1",
		"eventemitter3": "2.0.3"
	},
	"datav": {
		"cn_name": "平行光层",
		"protocol": 2,
		"config": {
			"handler": "updateOptions",
			"fold": true,
			"intensity": {
				"name": "光照强度",
				"type": "number",
				"default": 0.3,
				"range": [
					0,
					2
				],
				"step": 0.01
			},
			"color": {
				"type": "color",
				"name": "颜色",
				"default": "#FFFFFF"
			},
			"positionX": {
				"name": "位置X(经度)",
				"type": "number",
				"default": -110,
				"range": [
					-2000,
					2000
				],
				"step": 0.01
			},
			"positionY": {
				"name": "位置Y(纬度)",
				"type": "number",
				"default": 350,
				"range": [
					-2000,
					2000
				],
				"step": 0.01
			},
			"positionZ": {
				"name": "位置Z(高度)",
				"type": "number",
				"default": -330,
				"range": [
					-2000,
					2000
				],
				"step": 0.01
			}
		},
		"api_data": {},
		"apis": {},
		"type": [
			"map_3d_conference_subcoms"
		],
		"parent": {
			"supportTypes": [
				"map_3d_conference"
			],
			"supportNames": []
		},
		"icon": ""
	}
}