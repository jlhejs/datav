{
	"name": "datav-engine-ambientlighter",
	"version": "0.0.11",
	"main": "index.js",
	"dependencies": {
		"eventemitter3": "3.0.0"
	},
	"datav": {
		"cn_name": "环境光层",
		"protocol": 2,
		"config": {
			"handler": "updateOptions",
			"fold": true,
			"isSceneGroup": {
				"name": "场景分组",
				"type": "boolean",
				"default": false
			},
			"sceneName": {
				"show": [
					[
						"isSceneGroup",
						"$eq",
						true
					]
				],
				"description": "删除内容即可自定义添加场景",
				"type": "search",
				"name": "场景名",
				"default": "",
				"searches": [
					{
						"name": "主场景",
						"value": "main"
					},
					{
						"name": "场景一",
						"value": "scene1"
					},
					{
						"name": "场景二",
						"value": "scene2"
					},
					{
						"name": "场景三",
						"value": "scene3"
					},
					{
						"name": "场景四",
						"value": "scene4"
					},
					{
						"name": "场景五",
						"value": "scene5"
					}
				]
			},
			"intensity": {
				"name": "光照强度",
				"type": "number",
				"default": 0.3,
				"range": [
					0,
					10
				],
				"step": 0.01
			},
			"color": {
				"type": "color",
				"name": "颜色",
				"default": "#FFFFFF"
			}
		},
		"api_data": {},
		"apis": {},
		"publicHandler": {
			"show": {
				"description": "显示组件"
			},
			"hide": {
				"description": "隐藏组件"
			}
		},
		"type": [
			"map_3d_engine_subcoms"
		],
		"parent": {
			"supportTypes": [
				"map_3d_engine"
			],
			"supportNames": []
		},
		"icon": "https://img.alicdn.com/tfs/TB1NAr0ACzqK1RjSZFLXXcn2XXa-180-180.png"
	}
}