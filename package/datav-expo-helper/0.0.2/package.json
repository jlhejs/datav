{
	"name": "datav-expo-helper",
	"version": "0.0.2",
	"main": "index.js",
	"dependencies": {
		"chroma-js": "1.3.4",
		"safely-merge": "1.0.1",
		"eventemitter3": "2.0.3"
	},
	"datav": {
		"cn_name": "辅助坐标系",
		"protocol": 2,
		"config": {
			"handler": "updateOptions",
			"fold": true,
			"planeType": {
				"name": "辅助面方向",
				"type": "options",
				"default": "XZ",
				"options": [
					{
						"name": "XY面",
						"value": "XY"
					},
					{
						"name": "XZ面",
						"value": "XZ"
					},
					{
						"name": "YZ面",
						"value": "YZ"
					}
				]
			},
			"planeColor": {
				"type": "color",
				"name": "辅助面颜色",
				"default": "#ffff00"
			},
			"planeSize": {
				"name": "辅助面大小",
				"type": "number",
				"default": 100,
				"range": [
					10,
					1000
				],
				"step": 1
			},
			"axesLenght": {
				"name": "坐标系长度",
				"type": "number",
				"default": 200,
				"range": [
					10,
					1000
				],
				"step": 1
			}
		},
		"api_data": {},
		"apis": {},
		"type": [
			"map_3d_conference_subcoms"
		],
		"parent": {
			"supportTypes": [
				"map_3d_conference"
			],
			"supportNames": []
		},
		"icon": ""
	}
}