{
	"name": "map3d-earth-atmosphere",
	"version": "0.0.8",
	"main": "index.js",
	"dependencies": {
		"eventemitter3": "2.0.3"
	},
	"datav": {
		"cn_name": "大气层",
		"protocol": 2,
		"config": {
			"handler": "updateOptions",
			"fold": true,
			"atmosphereColor": {
				"type": "color",
				"name": "大气层颜色",
				"default": "#FFFFFF"
			},
			"atmosphereOpacity": {
				"name": "大气层透明度",
				"type": "number",
				"default": 0.8,
				"range": [
					0,
					1
				],
				"step": 0.01
			},
			"atmosphereIntensity": {
				"name": "大气层强度",
				"type": "number",
				"default": 1,
				"range": [
					1,
					2
				],
				"step": 0.01
			},
			"atmosphereScale": {
				"name": "大气层缩放",
				"type": "number",
				"default": 1.5,
				"range": [
					1.5,
					2
				],
				"step": 0.01
			}
		},
		"api_data": {},
		"apis": {},
		"publicHandler": {
			"show": {
				"description": "显示组件"
			},
			"hide": {
				"description": "隐藏组件"
			}
		},
		"type": [
			"map3d-earth-subcoms"
		],
		"parent": {
			"supportTypes": [
				"map_earth"
			],
			"supportNames": [
				"map_3d*"
			]
		},
		"icon": "https://img.alicdn.com/tfs/TB1IJ_7ArvpK1RjSZFqXXcXUVXa-180-180.png"
	}
}